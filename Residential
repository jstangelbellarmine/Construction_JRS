/*
 * Attributes:
 *  #numBedRooms: int
 *  #numBathrooms: int
 *  #laundryRoom: boolean
 *  
 *  methods:
 * +Residential()
 * +Residential(String projectName, String completeAddress, double totalSquareFeet, String occupancyGroup, string subgroup)
 * 
 * 
 */
public class Residential extends Building {

	protected int numBedrooms;
	protected int numBathrooms;
	protected boolean laundryRoom;
	
	public Residential()
	{
		
	}//ene open-argument constructor
	
	public Residential(String projectName, String completeAddress, double totalSquareFeet, String occupancyGroup, String subgroup)
	{
		
	}//end preferred constructor 	
	
	//getters/setters
	public int getNumBedrooms() {
		return numBedrooms;
	}//end getter

	public void setNumBedrooms(int numBedrooms) {
		this.numBedrooms = numBedrooms;
	}//end setter

	public int getNumBathrooms() {
		return numBathrooms;
	}//end getter

	public void setNumBathrooms(int numBathrooms) {
		this.numBathrooms = numBathrooms;
	}//end setter

	public boolean isLaundryRoom() {
		return laundryRoom;
	}//end getter

	public void setLaundryRoom(boolean laundryRoom) {
		this.laundryRoom = laundryRoom;
	}//end setter
	
	// end getters/setters
	
}
