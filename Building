
/* Attributes 
 * #projectName: String
 * #completeAddress: String
 * #totalSquareFeet: double
 * #occupancyGroup: String
 * #subgroup: String
 * 
 * Methods/Operations:
 * +Building()
 * +Building (String projectName, String completeAddress, doulbe totalSquareFeet, String occupancyGroup, String subgroup)
 * +draw(): void
 * +displayData(): String
 */	

public class Building {

	protected String projectName;
	protected String completeAddress;
	protected double totalSquareFeet;
	protected String occupancyGroup;
	protected String subgroup;
	public String displayData;
	
	
	//constructor
	
	public Building()
	{
		
	}//empty constructor
	
	public Building(String projectName, String completeAddress, double totalSquareFeet, String occupancyGroup, String subgroup)
	{
		
	}//preferred constructor 
	

		
	//start getter/setter
	public String getProjectName() {
		return projectName;
	}//end getter

	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}//end setter

	public String getCompleteAddress() {
		return completeAddress;
	}//end getter

	public void setCompleteAddress(String completeAddress) {
		this.completeAddress = completeAddress;
	}//end setter

	public double getTotalSquareFeet() {
		return totalSquareFeet;
	}//end getter

	public void setTotalSquareFeet(double totalSquiareFeet) {
		this.totalSquareFeet = totalSquareFeet;
	}//end setter

	public String getOccupancyGroup() {
		return occupancyGroup;
	}//end getter

	public void setOccupancyGroup(String occupancyGroup) {
		this.occupancyGroup = occupancyGroup;
	}//end setter

	public String getSubgroup() {
		return subgroup;
	}//end getter

	public void setSubgroup(String subgroup) {
		this.subgroup = subgroup;
	}//end setter

	public String getDisplayData() {
		return displayData;
	}//end getter

	public void setDisplayData(String displayData) {
		this.displayData = displayData;
	}//end setter


	
	public void draw()
	{
		
	}//not sure what this does? 
	
	public String displayData()
	{
		StringBuilder sb = new StringBuilder("Begin Data Report for Building Class\n\n");
		
		sb.append("Project Name: "+getProjectName()+"\n\n");
		sb.append("Address: "+getCompleteAddress()+"\n\n");
		sb.append("Square Feet: "+getTotalSquareFeet()+"\n\n");
		sb.append("Occupancy Group: "+getOccupancyGroup()+"\n\n");
		sb.append("Occupancy Subgroup: "+getSubgroup()+"\n\n");
		sb.append("End Data Report for Building Class. \n\n");
		return sb.toString();
		
	}//displayData string

	@Override
	public String toString() {
		return "Building [projectName=" + projectName + ", completeAddress=" + completeAddress + ", totalSquareFeet="
				+ totalSquareFeet + ", occupancyGroup=" + occupancyGroup + ", subgroup=" + subgroup + ", displayData="
				+ displayData + "]";
	}//end Building toString

	}
